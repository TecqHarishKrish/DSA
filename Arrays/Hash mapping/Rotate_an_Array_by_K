



# Rotate an  Array by K elements - O(n)=Time , O(1)=Space
#  Optimal approach 




class Solution:
    def rotateAnArrayByK(self, nums: List[int],k:int) -> List[int]:
        def reverse(nums,left,right):
            while left<right:
                nums[left],nums[right]=nums[right],nums[left]
                left+=1
                right-=1
        def rotate(nums,k):
            n=len(nums)
            if n<=1:
                return
            k%=n
            if k==0:
                return
            reverse(nums,0,n-1)
            reverse(nums,0,k-1)
            reverse(nums,k,n-1)
        rotate(nums,k)
        return nums
